\begin{frame}
    \frametitle{Model}
Modeling approach based on data characteristics
    \begin{itemize}
        \item Bivariate responses: ARTIST scores for CI and HT
        \item Binary curricula indicator
        \item Continuous covariates: pre-treatment scores
    \end{itemize} \pause
Multivariate Analysis of Covariance (MANCOVA) Model
\end{frame}

%-------------------------------------------------

\begin{frame}
    \frametitle{Model}

Bivariate MANCOVA Model Structure

\begin{eqnarray}\label{eq:mancova}
y_{ik} = \tau_{k} \mathbbm{1}_{\{i \in T\}} + \beta_{0k} + \sum_{p=1}^{P}x_{ip}\beta_{pk} + \epsilon_{ik},
\end{eqnarray}

 \begin{tabular}{lp{5in}}
 $y_{ik}$ &  $k^{th}$ response ($k \in \left\{1,2\right\}$) from  student $i$, $ 1 \le i \le n$,\\
 $\tau_{k}$ & treatment effect on response $k$, and \\
 $\mathbbm{1}_{\{i \in T\}}$ & indicator function student $i$ in Simulation-Based group\\
 $\beta_{0k}$ & common intercept for response $k$, and \\
 $\beta_{kp}$, & $1 \le p \le P$ model coefficients of $P$ covariates. \\
 $x_{ip}$ & $p^{th}$ pre-treatment covariate score of student $i$\\
 %$\epsilon}_{ik}$ & error for $k^{th}$ response from $i^{th}$ student. 
 \end{tabular}

\end{frame}

%-------------------------------------------------

\begin{frame}
    \frametitle{Model}

Bivariate MANCOVA Model Error Structure

\[
\vec{\epsilon}_{i} = 
\begin{bmatrix}
  \epsilon_{i1} \\ \epsilon_{i2} 
 \end{bmatrix}  
 \distas{iid} \text{MVN} \left( 
 \begin{bmatrix}
  0 \\ 0 
 \end{bmatrix},
 \Sigma = \begin{bmatrix}
  \sigma_{11}^2 & \sigma_{12}^2 \\ 
  \sigma_{21}^2 & \sigma_{22}^2
 \end{bmatrix}
 \right)
\]

\end{frame}

%-------------------------------------------------

\begin{frame}
    \frametitle{Model Selection}
   
Model Selection Process
    \begin{itemize}
        \item Bivariate MANCOVA model fit initially with all covariates
        \item Backward stepwise selection process based on AIC
        \item Additional covariates removed due to collinearity
    \end{itemize}
Selected Parameters for ARTIST Model
    \begin{itemize}
        \item Midterm exam score
        \item Lab 5 score
        \item Curriculum treatment effect
    \end{itemize}
\end{frame}

